<a class="list-group-item list-group-item-action" id="list-{{this.RoomID}}-list" data-bs-toggle="list" href="#list-{{this.RoomID}}" role="tab" aria-controls="list-{{this.RoomID}}"
    data-room-id="{{this.RoomID}}" data-room-name="{{this.ChatName}}">
    <div class="d-flex w-100 justy-content-between">
        <img src="{{this.Image}}"/>
        <h5 class="mb-1">{{this.ChatName}}</h5>
    </div>
</a>